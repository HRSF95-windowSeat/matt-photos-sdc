{
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "import",
        "jsx-a11y"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "react/prop-types": [
            2
        ],
        "react/forbid-prop-types": [
            "disabled",
            {
                "forbid": [
                    "string"
                ],
                "checkContextTypes": "boolean",
                "checkChildContextTypes": "boolean"
            }
        ]
    },
    "extends": [
        "airbnb"
    ]
}